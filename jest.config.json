{
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
        "src/**/*.js",
        "lib/**/*.js",
        "!**/node_modules/**",
        "!**/tests/**"
    ],
    "testMatch": [
        "**/tests/**/*.test.js"
    ],
    "transform": {},
    "moduleFileExtensions": [
        "js"
    ],
    "verbose": true
}